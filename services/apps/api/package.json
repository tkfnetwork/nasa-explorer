{
  "name": "@ne/api",
  "version": "1.0.0",
  "description": "API layer for interacting with the NASA API",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "pnpm generate:openapi-spec",
    "build": "rimraf dist && esbuild --bundle --platform=node --format=cjs ./src/index.ts --outfile=dist/server.js",
    "dev": "tsx --watch ./src/index.ts",
    "lint": "eslint --fix",
    "prettier": "prettier --write \"**/*.{ts,tsx,md,css,json}\"",
    "test": "jest",
    "generate:openapi-spec": "swagger-jsdoc -d ./src/features/docs/definition.yaml -o ./src/features/docs/openapi-spec.json ./src/**/*Controller.ts",
    "generate:postman": "pnpm generate:openapi-spec && portman -l ./src/features/docs/openapi-spec.json -o ./postman/collection.json -b http://localhost:3008"
  },
  "keywords": [],
  "author": "Kris Pomphrey <kris@tkf.network>",
  "license": "ISC",
  "packageManager": "pnpm@10.6.5",
  "engines": {
    "node": ">=22.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@faker-js/faker": "^9.9.0",
    "@ne/request-validation": "workspace:*",
    "@types/apicache": "^1.6.7",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-ws": "^3.0.5",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.10",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "esbuild": "^0.25.5",
    "eslint": "^9.30.1",
    "eslint-plugin-import": "^2.32.0",
    "factory.ts": "^1.4.2",
    "globals": "^16.3.0",
    "jest": "^30.0.4",
    "msw": "^2.10.3",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1"
  },
  "dependencies": {
    "@scalar/express-api-reference": "^0.8.9",
    "apicache": "^1.6.3",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "inversify": "^7.5.4",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.0",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "winston": "^3.17.0",
    "ws": "^8.18.3",
    "zod": "^3.25.73"
  }
}
